{
  "expo": {
    "name": "expo-template",
    "slug": "expo-template",
    "version": "1.0.0",
    "scheme": "expotemplate",
    "owner": "alefecrz",
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3C4D69",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "buildToolsVersion": "34.0.0",
      "compileSdkVersion": 34,
      "package": "com.expotemplate.app",
      "targetSdkVersion": 34
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": false
    },
    "orientation": "portrait",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-secure-store"
    ]
  }
}

